{{!-- articles below --}}

<div class="container">
  {{#each articles}}
  <div class="card">
  <h5 class="card-header"> Article #{{@index}}</h5>
  <div class="card-body">
    <h5 class="card-title">{{this.title}}</h5>
    <p class="card-text">With supporting text below as a natural lead-in to additional content.</p>
    <a href="{{this.link}}"><p class="card-text">Article Link</p></a>    
    <a href="#" class="btn btn-primary">Go somewhere</a>
  </div>
</div>
{{/each}}
</div>